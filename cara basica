function setup() { 
  createCanvas(600, 400);//Tamany de l'area de dibuix
  //El primer número el l'ampliada en pixels i el segon número es l'alçada en pixels.
  
  mic=new p5.AudioIn();//El micròfon es una entrada de audio amb p5 
  mic.start();//Activa el microfon

  function draw() {
  background(109, 191, 242);//Color del fons del dibuix.
//El primer número es el nivell de vermell"R", el segon número es el nivell de verd "G" i el tercer número es l'entensitat de blau "B", pero tant color RGB en html. Dintre de background o fons.
  fill(255, 246, 229);//Color del la cara exterior 
  //En el cas del fill será al mateix pero el que fa es omplir de color la figura següent que son dos ellipses
  elipse(300, 200, 200, 250);//Forma de la cara exterior
  //El primer número es la posicio 
